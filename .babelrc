{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "node": "current", "browsers": "> 0.25%, not dead" } }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
